import matplotlib.pyplot as plt
import seaborn as sns
import pandas as pd
import numpy as np
sns.set_theme(context="paper")
# Importing datasets
asylum_seekers = pd.read_csv("HK_asylum_stats.csv")
refugee = pd.read_csv("refugee.csv")
refugee = refugee.drop(columns = ["Indicator Name", "Indicator Code"]).melt(id_vars=["Country_Name", "Country_Code"], var_name="Year", value_name="refugees")
refugee["Year"] = refugee.Year.astype("int64")
refugee_hk = refugee.query("Year >=2005 and Country_Code == 'HKG'")
fig, axes = plt.subplots(1,2)
sns.lineplot(x="Year", y="Cases", data=asylum_seekers, linestyle = "--", ax=axes[0])
axes[0].set(ylabel="", title="Number of new non-refoulement claim\n cases in Hong Kong from 2005 to 2021", xticks=np.arange(2005, 2022, 4))
sns.lineplot(x="Year", y="refugees", data=refugee_hk, linestyle = ":", color="tab:orange", ax=axes[1])
axes[1].set(ylabel="", title="Number of refugees in Hong Kong\n from 2005 to 2020", xticks=np.arange(2005, 2021, 3))
plt.show()
plt.clf()
asylum_nationality = pd.read_excel("asylum_nationality_2021.xlsx")
asylum_nationality.sort_values("Number of claimants", inplace=True, ascending=False)
sns.barplot(y="Nationality", x="Number of claimants", data=asylum_nationality, color="tab:blue")
plt.title("Number of non-refoulement claimants by nationality in 2021")
plt.xlabel("Number of claimants")
plt.ylabel("")
plt.show()
plt.clf()
